# Frontend Environment Configuration

# Backend API URL - Using HTTP for development (no HTTPS certificate issues)
REACT_APP_API_URL=http://localhost:5074/api

# Enable detailed API logging in development
REACT_APP_ENABLE_API_LOGS=true

# App configuration
REACT_APP_NAME=Requirements Quality Analyzer
REACT_APP_VERSION=1.0.0

# Feature flags
REACT_APP_ENABLE_MOCK_FALLBACK=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_MAX_BATCH_SIZE=50

# Development settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=true